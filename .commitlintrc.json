{
    "extends":["@commitlint/config-conventional"],
    "rules": {
        "type-enum": [2, "always", ["feat", "fix", "docs", "style", "refactor", "test", "revert"]],
        "references-empty": [2, "never"]
    },
    "parserPreset": {
        "parserOpts": {
            "issuePrefixes": ["SGRS-"]
        }
    }
}